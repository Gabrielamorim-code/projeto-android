(function (){document.addEventListener("DOMContentLoaded",function (){const blogContainer=document.querySelector('.proposed-sets__swiper');const breakpoint=window.matchMedia('(min-width:1025px)');let mySwiper;if (blogContainer){function enableSwiper(){if (blogContainer){mySwiper=new Swiper(blogContainer,{direction:'horizontal',spaceBetween:8,grabCursor:true,slidesPerView:'auto',});};};const breakpointChecker=()=>{if (breakpoint.matches===true){if (mySwiper){mySwiper.destroy(true,true);};}else if (breakpoint.matches===false){enableSwiper();};};breakpoint.addListener(breakpointChecker);breakpointChecker();};});})();