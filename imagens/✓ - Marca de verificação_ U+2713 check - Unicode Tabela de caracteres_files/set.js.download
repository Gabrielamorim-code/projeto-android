(function(){var textArea;function isOS(){return navigator.userAgent.toLowerCase().match(/ipad|iphone/i);};function createTextArea(text){textArea=document.createElement('textArea');textArea.setAttribute('readonly',true);textArea.style.position='fixed';textArea.value=text;document.body.appendChild(textArea);};function selectText(){var range,selection;if (isOS()){range=document.createRange();range.selectNodeContents(textArea);selection=window.getSelection();selection.removeAllRanges();selection.addRange(range);textArea.setSelectionRange(0,999999);}else{textArea.select();};};function copyToClipboard(){document.execCommand('copy');document.body.removeChild(textArea);};function copy(text){createTextArea(text);selectText();copyToClipboard();};document.addEventListener('DOMContentLoaded',function (){document.addEventListener('click',function (evt){if (evt.target.classList.contains('js-symbol-copy')){let symbol=evt.target.dataset.symbol;if (typeof symbol==='undefined'){symbol=evt.target.dataset.copy;};copy(symbol);const setPopup=document.querySelector('.set-popup');if (setPopup){setPopup.classList.add('set-popup--active')
setPopup.querySelector('.set-popup__symbol').textContent=symbol;setTimeout(function(){setPopup.classList.remove('set-popup--active')},1000);};};});});})();